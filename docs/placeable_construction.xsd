                                    </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="node" type="g_node_index">
                                    <xs:annotation>
                                        <xs:documentation xml:lang="en">Spike node</xs:documentation>
                                    </xs:annotation>
                                </xs:attribute>
                            </xs:complexType>
                        </xs:element>
                    </xs:choice>
                    <xs:attribute name="maxRot" type="g_angle">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Max. spike rotation</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="moveDownRange" type="g_vector_rot_2">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Move down range</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="moveUpRange" type="g_vector_rot_2">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Move up range</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="rotAxis" type="g_int" default="3">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Rotation axis</xs:documentation>
                        </xs:annotation>
                    </xs:attribute>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute name="class" type="g_string" default="RotationAnimation">
            <xs:annotation>
                <xs:documentation xml:lang="en">Animation class (RotationAnimation | RotationAnimationSpikes | ScrollingAnimation | ShakeAnimation)</xs:documentation>
            </xs:annotation>
