                                                                    <xs:documentation xml:lang="en">Index of corresponding animated object in xml</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                            <xs:attribute name="startTime" type="g_float" use="required">
                                                                <xs:annotation>
                                                                    <xs:documentation xml:lang="en">Normalized start time to activate obstacle</xs:documentation>
                                                                </xs:annotation>
                                                            </xs:attribute>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:all>
                                                <xs:attribute name="node" type="g_node_index" use="required">
                                                    <xs:annotation>
                                                        <xs:documentation xml:lang="en">Node to be used for obstacle box</xs:documentation>
