            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="volumeScale" type="g_float" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">Additional scale that is applied on the volume attributes</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AnimationNode">
        <xs:all>
            <xs:element name="spikes" minOccurs="0" maxOccurs="1">
