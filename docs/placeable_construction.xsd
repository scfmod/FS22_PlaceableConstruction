                <xs:documentation xml:lang="en">Min. alpha for turn off (speed [0-1])</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="node" type="g_node_index">
            <xs:annotation>
                <xs:documentation xml:lang="en">Node</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotAxis" type="g_float" default="2">
            <xs:annotation>
                <xs:documentation xml:lang="en">Rotation axis</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="rotSpeed" type="g_angle" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">Rotation speed</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="scrollLength" type="g_float" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">(ScrollingAnimation) Scroll length</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="scrollSpeed" type="g_float" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">(ScrollingAnimation) Scroll speed</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderComponentScale" type="g_vector_4" default="1 0 0 0">
            <xs:annotation>
                <xs:documentation xml:lang="en">Shader parameter name</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterComponent" type="g_float" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">(ScrollingAnimation) Shader parameter component</xs:documentation>
            </xs:annotation>
        </xs:attribute>
