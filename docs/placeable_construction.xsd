                <xs:documentation xml:lang="en">Turn off time variance</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="type" type="g_string">
            <xs:annotation>
                <xs:documentation xml:lang="en">(ScrollingAnimation) Fill type name</xs:documentation>
            </xs:annotation>
        </xs:attribute>
