        <xs:attribute name="outerRadius" type="g_float" default="5">
            <xs:annotation>
                <xs:documentation xml:lang="en">Outer radius</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="parent" type="g_string">
            <xs:annotation>
                <xs:documentation xml:lang="en">Parent sample for heredity</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="pitchScale" type="g_float" default="1">
            <xs:annotation>
                <xs:documentation xml:lang="en">Additional pitch that is applied on the volume attributes</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="supportsReverb" type="g_bool" default="true">
            <xs:annotation>
                <xs:documentation xml:lang="en">Flag to disable reverb</xs:documentation>
            </xs:annotation>
