        <xs:attribute name="fadeOut" type="g_float" default="0">
            <xs:annotation>
                <xs:documentation xml:lang="en">Fade out time in seconds</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="file" type="g_string">
            <xs:annotation>
                <xs:documentation xml:lang="en">Path to sound sample</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="innerRadius" type="g_float" default="80">
            <xs:annotation>
                <xs:documentation xml:lang="en">Inner radius</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="isLocalSound" type="g_bool" default="false">
            <xs:annotation>
                <xs:documentation xml:lang="en">While set for vehicle sounds it will only play for the player currently using the vehicle</xs:documentation>
