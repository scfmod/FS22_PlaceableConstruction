                                                        <xs:documentation xml:lang="en">Maximum vehicle width supported by the spline</xs:documentation>
                                                    </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="node" type="g_node_index">
                                                    <xs:annotation>
