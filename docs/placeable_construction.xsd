            <xs:annotation>
                <xs:documentation xml:lang="en">Shader parameter name</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="shaderParameterPrevName" type="g_string" default="automatically calculated from #shaderParameterName">
